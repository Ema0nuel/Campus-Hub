{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "devCommand": "npm run dev",
  "framework": "svelte",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "status": 200,
      "dest": "/"
    }
  ],
  "functions": {
    "api/**.js": {
      "memory": 128,
      "maxDuration": 10
    }
  },
  "env": {
    "VITE_SUPABASE_URL": "@supabase_url",
    "VITE_SUPABASE_ANON_KEY": "@supabase_anon_key",
    "VITE_SUPABASE_BUCKET_NAME": "avatars"
  }
}
